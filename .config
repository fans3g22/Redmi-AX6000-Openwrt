CONFIG_HAS_SUBTARGETS=y
CONFIG_HAS_DEVICES=y
CONFIG_TARGET_BOARD="mediatek"
CONFIG_TARGET_SUBTARGET="filogic"
CONFIG_TARGET_PROFILE="DEVICE_xiaomi_redmi-router-ax6000-stock"
CONFIG_TARGET_ARCH_PACKAGES="aarch64_cortex-a53"
CONFIG_DEFAULT_TARGET_OPTIMIZATION="-Os -pipe -mcpu=cortex-a53"
CONFIG_CPU_TYPE="cortex-a53"
CONFIG_LINUX_6_12=y
CONFIG_DEFAULT_base-files=y
CONFIG_DEFAULT_bridger=y
CONFIG_DEFAULT_ca-bundle=y
CONFIG_DEFAULT_dnsmasq-full=y
# --- 核心组件 ---
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-ssl=y

# 用户列表明确包含 nginx-ssl，建议禁用 uhttpd 改用 nginx，如下：
# CONFIG_PACKAGE_uhttpd is not set
CONFIG_PACKAGE_nginx-ssl=y
CONFIG_PACKAGE_luci-nginx=y
CONFIG_PACKAGE_luci-ssl-nginx=y

# --- 特殊功能插件 (基于您的列表) ---
CONFIG_PACKAGE_luci-app-adguardhome=y  # 广告拦截
CONFIG_PACKAGE_adguardhome=y
CONFIG_PACKAGE_luci-app-nikki=y        # Nikki (代理工具)
CONFIG_PACKAGE_nikki=y
CONFIG_PACKAGE_luci-app-turboacc=y     # 网络加速 (Flow Offload)
CONFIG_PACKAGE_kmod-nft-offload=y      # 配合 TurboACC

CONFIG_PACKAGE_luci-app-filemanager=y  # 文件管理
CONFIG_PACKAGE_luci-app-log-viewer=y   # 日志查看
CONFIG_PACKAGE_luci-app-syscontrol=y   # 系统控制
CONFIG_PACKAGE_luci-app-fan=y          # 风扇控制 (AX6000 通常是被动散热，但如果是改装版可能需要)
CONFIG_PACKAGE_luci-app-upnp=y         # UPnP

# --- 实用工具 ---
CONFIG_PACKAGE_luci-app-ttyd=y
CONFIG_PACKAGE_ttyd=y
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_curl=y
CONFIG_PACKAGE_wget-ssl=y
CONFIG_PACKAGE_nano=y
CONFIG_PACKAGE_yq=y                   # YAML 处理工具
CONFIG_PACKAGE_jq=y                   # JSON 处理工具(通常 implied)

# --- 网络与协议 ---
CONFIG_PACKAGE_ip-full=y
CONFIG_PACKAGE_bind-dig=y
CONFIG_PACKAGE_openssh-sftp-server=y
CONFIG_PACKAGE_wireguard-tools=y
CONFIG_PACKAGE_luci-proto-wireguard=y

# --- Wi-Fi 安全与驱动 ---
# 解决冲突: 移除基础版 wpad，使用功能最全的 openssl 版
# CONFIG_PACKAGE_wpad-basic-mbedtls is not set
# CONFIG_PACKAGE_wpad-basic-wolfssl is not set
# CONFIG_PACKAGE_wpad-mbedtls is not set
CONFIG_PACKAGE_wpad-openssl=y
CONFIG_PACKAGE_hostapd-common=y

# --- 依赖库 (Usually selected automatically, but ensuring) ---
CONFIG_PACKAGE_libcap=y
CONFIG_PACKAGE_libcurl4=y
CONFIG_PACKAGE_libwebsockets-full=y

# --- 修复/默认 ---
# CONFIG_PACKAGE_dnsmasq is not set
CONFIG_PACKAGE_dnsmasq-full=y
CONFIG_PACKAGE_ipv6helper=y

# --- 主题 (根据列表) ---
CONFIG_PACKAGE_luci-theme-bootstrap=y
CONFIG_PACKAGE_luci-theme-openwrt=y
# 您的列表里有 luci-app-advancedplus (Kucat 设置)，如果您需要 Kucat 主题，请手动把下面这行前面的 # 去掉
# CONFIG_PACKAGE_luci-theme-kucat=y
